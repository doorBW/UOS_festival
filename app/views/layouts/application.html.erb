      }
      .sche_ball{
        display : inline-block;
      }
      .sche_content_box{
        display: flex;
        width:  250px;
        height: 250px;
        flex-direction: column;
        align-items : center;
        background-repeat : no-repeat; 
        background-size : cover;
        justify-content : space-around;
        border-radius : 100%;
        border: 5px solid white;
